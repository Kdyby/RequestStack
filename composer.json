{
	"name": "kdyby/request-stack",
	"type": "library",
	"description": "Http Request stack service that wraps the Request value object for Nette Framework",
	"keywords": ["nette", "kdyby", "http", "request", "stack"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Filip Proch√°zka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		}
	],
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/Kdyby/RequestStack/issues"
	},
	"require": {
		"php": "^5.6 || ^7.0",
		"nette/di": "^2.4.10@dev",
		"nette/http": "^2.4@dev",
		"kdyby/strict-objects": "^1.0"
	},
	"require-dev": {
		"nette/application": "^2.4@dev",
		"nette/bootstrap": "^2.4@dev",
		"nette/tester": "^2.0"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"Kdyby\\RequestStack\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"KdybyTests\\": "tests/KdybyTests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.3-dev"
		}
	}
}
